{
    "version": "0.1.0",
    "command": "g++-6",
    "args": [
        "-Wall",
        // "-O2",
        "-std=c++14",
        "${file}",
        "-o",
        "${workspaceRoot}/Debug/debug"
    ],
    "problemMatcher": {
        "owner": "cpp",
        "fileLocation": [
            "relative",
            "${workspaceRoot}"
        ],
        "pattern": {
            "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
        }
    }
}